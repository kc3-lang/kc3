require Marshall
Marshall
require MarshallRead
MarshallRead
loopback = fn (tag) do
  str = Marshall.tag_to_str(tag)
  result = MarshallRead.tag_from_str(str)
  if (inspect(result) == inspect(tag)) do
    {:ok, Str.size(str)}
  else
    {:ko, Str.size(str), result}
  end
end
