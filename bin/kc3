#!/usr/bin/env kc3s

[argv0, op | args] = args()

def usage = fn () {
  puts("Usage: #{File.name(argv0)} COMMAND [ARGUMENT ...]
 Where COMMAND can be one of :
  help [CMD]            Show a longer help message
  new TYPE NAME         Create a new project
")
  exit(1)
}

puts("Op: #{op}")
puts("Args: #{args}")

exit(0)
