#!/bin/sh
{
    echo "# generated by configure"
    PROJECT_DIR="$PWD"
    echo "PROJECT_DIR = ${PROJECT_DIR}"

    if [ "x${BUILD_TYPE}" = "x" ]; then
        BUILD_TYPE=debug
        APK_TYPE=$(echo ${BUILD_TYPE} | tr '[A-Z]' '[a-z]')
    fi
    echo "BUILD_TYPE = ${BUILD_TYPE}"
    echo "APK_TYPE = ${APK_TYPE}"

} > configure.mk
