%{module: C3.Facts.Dump,
  version: 1}
add {List, :is_a, :module}
add {List, :symbol, List.map}
add {List, :symbol, List.reverse}
replace {List.map, :fn, fn {
  ((), _) {
    ()
  }
  ((a | b), f) {
    (f(a) | List.map(b, f))
  }
}}
replace {List.reverse, :fn, fn {
  (x) { List.reverse(x, ()) }
  ((), acc) { acc }
  ((a | b), acc) { List.reverse(b, (a | acc)) }
}}
